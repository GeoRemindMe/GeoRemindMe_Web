body{
    color: #333333;
    direction: ltr;
    font-family: "lucida grande",tahoma,verdana,arial,sans-serif;
    font-size: 11px;
    text-align: left;
    margin:0;
}
input{
    font-family: "lucida grande",tahoma,verdana,arial,sans-serif;
}
#container {
  width: 760px;
  margin: 0 auto;
}

/****************************
 *      
 *  Links
 * 
 ***************************/
#facebookApp .link,
#facebookApp .hoverlink,
#facebookApp #view-suggestion span.likeBtn span.hoverlink,
#facebookApp ul#chronology a.link{
    color:blue !important;
}

.link{
    text-decoration:underline;
    cursor:pointer;
}
.hoverlink{
    text-decoration:none;
}
.hoverlink:hover{
    text-decoration:underline;
    cursor:pointer;
}
.black-link{
    color:black !important;
}
#view-suggestion span.likeBtn span.hoverlink{
    text-decoration:none;
}
#view-suggestion span.likeBtn span.hoverlink:hover{
    text-decoration:underline;
}

#details-form span.used{
    color: #555555 !important;
    cursor: pointer;
}

/****************************
 *      
 *  LEFT COLUMN
 * 
 ***************************/
#left-col{
    
    
    float: left;
    width: 180px;
}
    div#left-col a#logo img{
        border:0;
        padding-top: 0.5em;
        width:170px;
    }
    
    div#left-col ul{
        list-style: none outside none;
        margin: 0;
        padding: 0;
    }
    div#left-col li{
/*
        padding: 5px 0;
*/
        
    }
    /*
      Friend to follow  
    */
    div#left-col div.help-icon {
        margin: 4px 7px 0 0;
    }
    div#left-col ul#friends-to-follow{
        margin: 1em 0 0;
    }
    div#left-col ul#friends-to-follow:after{
            clear: both;
            content: ".";
            display: block;
            height: 0px;
            visibility: hidden;
        }
    div#left-col ul#friends-to-follow li {
        clear: left;
        margin: 0 0.2em;
        padding: 0.5em 0;
        width: auto;
    }
        div#left-col ul#friends-to-follow li a{
            background-position: 4px 3px;
            padding: 1px 7px 1px 22px;
            width: auto;
            
        }
        div#left-col ul#friends-to-follow li div.avatar a.user-img{
            float: left;
            margin-right: 10px;
            padding: 0;
        }
        div#left-col ul#friends-to-follow li div.avatar a.user-img img{
            height:25px;
        }
        div#left-col ul#friends-to-follow li div.avatar div.follow-button{
            margin-top: 2px;
        }
        div#left-col ul#friends-to-follow li div.avatar span.waiting {
            background-position: 4px 3px;
            padding: 1px 7px 1px 22px;
            width: auto;
        }
        div#left-col ul#friends-to-follow li div.avatar span.waiting a{
            background:none;
            padding-left:6px;
        }
        div#left-col ul#friends-to-follow li div.avatar div.removable{
            margin:5px;
        }
        div#left-col ul#friends-to-follow li div.avatar div.removable a{
            display:none;
        }
    
    div#left-col li a{
        color: black;
        display: block;
        line-height: 19px;
        padding: 5px 0 5px 30px;
        text-decoration: none;
        width: 150px;
    }
    div#left-col li:hover{
        background-color:#eff2f7
    }
    
    .install-icons a {
        background-image: url("/static/facebookApp/img/intro_icons.png");
    }
    
    #introduction-icon{
        background-position: 5px 5px;
    }
    #about-us-icon{
        background-position: 5px -20px;
    }
    #community-icon{
        background-position: 5px -45px;
    }
    #blog-icon{
        background-position: 5px -70px;
    }
    #privacy-icon{
        background-position: 5px -95px;
    }
    #tos-icon{
        background-position: 5px -120px;
    }
    
    
    
    #fb-wall {
        background-position: 5px 5px;
    }
    #fb-last-news {
        background-position: 5px -20px;
    }
    #fb-events {
        background-position: 5px -45px;
    }
    #fb-grm {
        background-position: 5px -70px;
    }
    
    
    .menu-icons a{
        background-repeat: no-repeat;
        display: inline-block;
        height: 16px;
        width: 17px;
    }

    .fb-icons a {
        background-image: url("/static/facebookApp/img/fb-icons.png");
    }
    
    .main-menu-icons a {
        background-image: url("/static/facebookApp/img/main_menu_icons.png");
    }
    
    #activity-icon {
        background-position: 5px 5px;
    }
    #vault-icon {
        background-position: 5px -22px;
    }
    #suggestions-icon {
        background-position: 5px -45px;
    }
    #notifications-icon {
        background-position: 5px -70px;
    }
    #places-icon {
        background-position: 4px -95px;
    }
    #profile-icon {
        background-position: 5px -120px;
    }
    #settings-icon {
        background-position: 5px -145px;
    }

/****************************
 *      
 *  RIGHT COLUMN
 * 
 ***************************/
#right-col{
    border-left: 1px solid #B3B3B3;
    float: left;
    padding-left: 15px;
    padding-bottom: 15px;
    width: 550px;
    min-height:500px;
    font-size: 1em;
    line-height: 1.5em;
}

    #right-col #register-panel h1{
        border-bottom: 2px solid #CCCCCC;
        font-size: 2em;
        padding: 5px 0 20px;
        text-align: left;
    }
    #right-col h2{
        font-size:1.3em;
        
    }
    #right-col #register-panel{
        text-align:center;
    }
    #right-col #register-panel p{
        font-size: 1em
    }
    #right-col #register-panel #video{
        clear: both;
        margin: 0 auto 30px;
        width: 399px;
    }
    #right-col .left{
        float:left;
    }
    #right-col .right{
        float:right;
    }
    
    
    /****************************
     *      
     *  COMMON
     * 
     ***************************/
    .suggestionx16{
        background:url("/static/common/img/suggestionsx16.png") no-repeat;
        padding-left: 20px;
    }
    .like-dislike,.likex16{
        display: inline-block;
        line-height: 20px;
/*
        padding-left: 20px; Ya no es necesaria en view suggestion
*/
    }
    .like-dislike img,.likex16 img{
        background: url("/static/webapp/img/grm_webapp_sprite.png") no-repeat scroll -53px -3px transparent;
        height: 16px !important;
        margin: 0 !important;
        width: 16px !important;
    }
    .reject-suggestion{
        background:url("/static/common/img/reject.png") no-repeat scroll 0 -2px transparent;
        padding-left: 20px;
    }
    .accept-suggestion{
        background:url("/static/common/img/accept.png") no-repeat scroll 0 -2px transparent;
        padding-left: 20px;
    }
    .remember-forget img,
    .saved img{
        background-position: -77px -27px;
    }
	#print-icon img{
		background: url("/static/webapp/img/grm_webapp_sprite.png") no-repeat scroll -150px -83px transparent;
        border: 0 none !important;
        clear: none !important;
        float: left !important;
        height: 16px;
        margin: 0 !important;
        padding: 0 5px 0 0;
        width: 18px;
	}
    #notification-msg{
        border: 1px dotted #CCCCCC;
        left: 50%;
        line-height: 2em;
        margin-left: -200px;
        padding: 0.5em 1em;
        position: absolute;
        width: 400px;
    }
    .strong{
        font-weight:bold;
    }
    .error{
        background-color: #FCC7C7;
        
    }
    .success{
        background-color: #C4FF89;
        
    }
    
    #right-col h1{
        color: #555555;
        display: inline;
        font-family: Helvetica Neue,Helvetica,sans-serif;
        font-size: 1.7em;
        font-weight: normal;
        line-height: 2.5em;
    }
    #right-col .section{
        clear:both;
    }
    #right-col .section .header, .right-col .section .article .header{
        border-bottom: medium none;
        border-top: 1px solid #E2E2E2;
        background-color:#F2F2F2;
        padding:0 5px;
        height:30px;
    }
    #right-col .section p, #right-col .section h3{
        padding:0 5px;
    }
    #right-col .section .header p, .right-col .section .article .header p{
        float:right;
    }
    #right-col .section .header h2,#right-col .section .article .header h2{
        font-size:1em;
        float:left;
    }
    .error-msg{
         display: none;
    }
    
    #edit-profile .error-msg{
         display: block;
    }
    .error-msg ul{
    }
    .error-msg ul li{
    }
    div#left-col hr,#right-col hr,hr{
        border: 1px solid #D3D3D3;
        clear:both;
    }
    
    input[type="submit"], input[type="button"]{
        border-style: solid;
        border-width: 1px;
        border-color: #D9DFEA #0E1F5B #0E1F5B #D9DFEA;
        background-color: #3B5998;
        color: white;
        padding: 2px 15px 3px 15px;
        text-align: center;
        float:right
    }

    input[type="submit"]:hover, input[type="button"]:hover{
        cursor:pointer;
    }
    .footer-notes{
        color: #777;
        font-size:0.9em
    }
    .note{
        color: #777;
        font-size:0.9em;
        border:1px solid #ccc;
        padding: 5px;
        margin: 10px 0;
    }
    .note a{
        color: inherit;
        padding-left: 10px;
    }
    #active-section,div #left-col li#active-section:hover{
        background-color: #D8DFEA;
    }
    .header p {
        margin:5px;
    }
    img{
        border:0
    }
    
    .follow-button{
        -moz-border-radius: 4px;
        -khtml-border-radius: 4px;
        -webkit-border-radius: 4px;
        background: #DDD url(../img/buttons/bg-btn.gif) repeat-x 0 0;
        border-color: #BBB #BBB #999;
        border-radius: 4px;
        border-style: solid;
        border-width: 1px;
        cursor: pointer;
        display: inline-block;
        margin: 0;
        overflow: hidden;
        padding: 0;
        text-shadow: 0 1px #F0F0F0;
        color: #333!important;
        -moz-box-shadow: 0 1px 0 #F8F8F8;
        -wekbit-box-shadow: 0 1px 0 #F8F8F8;
        box-shadow: 0 1px 0 #F8F8F8;
        background-image: url("/static/facebookApp/img/btn_follow_bg.png");
        background-repeat:norepeat;
        
    }
    .follow-button a,
    .follow-button a:visited
    {
		background-image: url("/static/facebookApp/img/sprite_follow.png");
		background-repeat: no-repeat;
		color: black;
		font-family: Helvetica Neue,Arial,"Lucida Grande",Sans-serif;
		font-weight: bold;
		line-height: 23px;
		padding: 7px 7px 7px 22px;
		text-decoration: none;
    }
    .follow-button a.no-following{
        background-position: 5px 6px;
    }
    .follow-button a.following{
        background-position: 4px -60px;
    }
    
    .following-state{
        background-position:0 -66px;
    }
    span.waiting{
        background: url("/static/facebookApp/img/spinning-icons/wait16trans.gif") no-repeat scroll 3px 5px transparent;
        color: black;
        display: block;
        font-weight: bold;
        padding: 0px 7px 0px 22px;
        line-height: 2em;
    }

    .following-state span a.following{
        color:white;
    }
    
        
    .following-state:hover{
        background-position:0 -133px;
    }
    .following-state span a.following:hover{
        background-position:3px -25px;
        
    }
    div.help-icon{
        float:right;
        height: 22px;
        margin: 6px 0;
    }
    #add-suggestion div.help-icon{
        margin-top:0;
    }
    
    div#content div.expanded div.help-icon{
        border-bottom:0;
        margin:0;
        display: inline;
        float: none;
    }
    div#content div.expanded div.help-icon img{
    height: 13px;
    width: 13px;
    }
    #activity div.help-icon{
        height: 16px;
        margin: 15px 0;
    }
    
    div.help-icon img{
        width:16px;
        height:16px;
        cursor:pointer;
        background:url("/static/webapp/img/grm_webapp_sprite.png") repeat scroll -3px -4px transparent;
        
    }
    #btn-add-suggestion{
        -moz-border-radius: 3px 3px 3px 3px;
        -webkit-border-radius: 3px;
        border-radius: 3px;
        background: -moz-linear-gradient(center top , #F9F9F9, #E3E3E3) repeat scroll 0 0 transparent;
        background: -webkit-gradient(linear,0% 40%,0% 70%,from(#F9F9F9),to(#E3E3E3));
        background-image: -o-linear-gradient(top, rgb(249, 249, 249) 40%, rgb(227, 227, 227) 70%);
        
        border-top-color: #CCC;
        border: 1px solid #BBB;
        border-bottom-color: #A0A0A0;
        color: #000000;
        cursor: pointer;
        display: block;
        font: 1.2em arial,sans-serif;
        margin: 10px;
        outline: medium none;
        padding: 5px 8px;
        text-align: center;
        text-decoration: none;
        vertical-align: middle;
        white-space: nowrap;    
    }
    .clear{
        clear:both;
    }
    #go-back{
        cursor:pointer;
        color:#3896FF;
        text-decoration:underline;
        float:right;
    }
    input[type="submit"].waiting{
        background: url("/static/facebookApp/img/spinning-icons/wait16.gif") no-repeat scroll 4px 3px white;
        border: 2px dotted #CCCCCC;
        color: black;
        padding-left: 20px;
    }
    .show-more{
        text-decoration:underline;
        cursor:pointer;
        float:right;
    }
    #show-more-text{
        display:none;
        
    }
    #social-profile #show-more-text{
        line-height:2em;
    }
    
    a:visited, a:active{
        color:#01F;
    }
    
    .btn{
        background-color: #5973A8;
        border-color: #D9DFEA #0E1F5B #0E1F5B #D9DFEA;
        border-style: solid;
        border-width: 1px;
        color: white;
        cursor: pointer;
        float: right;
        font-size: 13px;
        font-weight: bold;
        line-height: 20px;
        padding: 1px 15px 2px;
        text-align: center;
    }
    div#add-suggestion div span#submit-button.btn{
        margin-right:1em;
    }
    .hidden{
        visibility:hidden;
        display:none;
    }
    .visible{
        visibility:visible;
    }
    .visible-display{
        visibility:visible !important;
        display:block !important;
    }

    
    
    #answerMessage{
        display:none;
        clear: both;
        font-size: 1.1em;
        margin: 1em 0;
        padding: 0.5em;
        position: relative;
        text-align: center;
        top: 5px;
    }
    
    .min-img{
        width:25px;
        height:25px;
        float:left;
    }
    
    div.removable{
        width:15px;
        height:15px;
        background: url("/static/webapp/img/grm_webapp_sprite.png") repeat scroll -4px -50px transparent;
        float:right;
        cursor:pointer;
        margin-right: -72px;
/*
        margin-top:0.5em;
*/
    }
    .comment-box div.removable{
        margin-right:0;
    }
    
    div.removable:hover{
        background-position: -4px 40px;
    }
    div.removable a{
        display:none;
    }
    .small-text{
            font-size:0.9em;
            color: #555555;
        }
    a.small-link{
        font-weight:bold;
        color: #555555;
        text-decoration:none;
    }
    a.small-link:hover{
        text-decoration:underline;
    }
    
    ul#chronology{
        font-size: 1.1em;
        line-height: 1.7em;
        margin: 0 5px;
        padding: 0;
    }
        ul#chronology .suggestionPlace{
/*
            clear: both; Para evitar descuadrar el timeline
*/
            color: #555555;
            display: block;
            font-size: 0.9em;
            
        }
        .suggestionPlace{
            line-height:20px;
            display:inline-block;
        }
        ul#chronology .suggestionPlace a{
            color: #555555;
/*
            font-weight:bold;
*/
            text-decoration:none;
        }
        ul#chronology .suggestionPlace a:hover{
            text-decoration:underline;
        }
        ul#chronology li{
            border-bottom: 1px solid #DDDDDD;
            clear: both;
            list-style: none outside none;
            margin: 0.3em 0;
            padding: 1em 0;
        }
        ul#chronology li:last-child{
            border:0;
        }
        
        ul#chronology li:after{
            clear: both;
            content: ".";
            display: block;
            height: 0px;
            visibility: hidden;
        }
        ul#chronology li hr{
        }
        
        ul#chronology li div.avatar{        
            border-bottom: 0 none;
            display: block;
            float: left;
            height: 50px;
            margin: 0.5em 0.7em 0 0.3em;
            text-align: left;
            width: 50px;
        }
        ul#chronology li div.avatar img{
            width:50px;
            height:50px;
            border-radius: 5px;
            -moz-border-radius: 5px;
        }
        
        ul#chronology li a.black-link,a.black-link,a.black-link:visited{
            text-decoration:none;
            color:#333 !important;
        }
        ul#chronology li a.dark-link,a.dark-link{
            color:#555555 !important;
            text-decoration:none;
            
        }
        ul#chronology li .timeline-msg img{
            float: left;
            height: 25px;
            margin-right: 10px;
            width: 25px;
        }
        ul#chronology li a.black-link:hover{
            text-decoration:underline;
        }
        ul#chronology li span.action-bar{
            font-size: 0.9em;
            padding-left: 0.5em;
            visibility:hidden;
        }
        ul#chronology li span.action-bar span{
            
        }
        
        ul#chronology li span.action-bar span.comment{
            background:url("/static/common/img/comments.png") no-repeat;
            padding-left:20px;
            display: inline-block;
            height: 16px;
            line-height: 16px;
        }
        ul#chronology li span.action-bar .details{
            
            display: inline-block;
            height: 16px;
            line-height: 16px;

        }
        ul#chronology li span.action-bar .details img{
            background:url("/static/webapp/img/grm_webapp_sprite.png") no-repeat scroll -102px -2px transparent;
            width:16px;
            height:16px;
            margin-right: 5px;
        }
        
            ul#chronology li .comment-box .action-bar{
                font-size:1em;
            }
