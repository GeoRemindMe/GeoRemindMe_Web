}

/*
 *
 * HELP LAYOUT (BOXES & ARROWS)
 *  
 */

.arrow{
	border-bottom: 10px solid transparent;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-top: 10px solid transparent;
	width: 0;
	height: 0;
	position: absolute;
}
.arrow-up {
	border-bottom: 10px solid #1C1C1C;
	border-top:0;
}
.arrow-down {
	border-top: 10px solid #1C1C1C;
	border-bottom: 0;
}
.arrow-right {
	border-left: 10px solid #1c1c1c;
	border-right: 0;
}

.arrow-left {
	border-right: 10px solid #1C1C1C;
	border-left: 0;
}
.help-box{
	padding: 0 5px;
	width: 250px;
	margin: 15px;
	
	color:white;
	background: #5798be; /* Old browsers */
	background: -moz-linear-gradient(top, #5798be 0%, #0a6a9f 3%, #04344f 71%, #04344f 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5798be), color-stop(3%,#0a6a9f), color-stop(71%,#04344f), color-stop(100%,#04344f)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #5798be 0%,#0a6a9f 3%,#04344f 71%,#04344f 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #5798be 0%,#0a6a9f 3%,#04344f 71%,#04344f 100%); /* Opera11.10+ */
	background: -ms-linear-gradient(top, #5798be 0%,#0a6a9f 3%,#04344f 71%,#04344f 100%); /* IE10+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5798be', endColorstr='#04344f',GradientType=0 ); /* IE6-9 */
	background: linear-gradient(top, #5798be 0%,#0a6a9f 3%,#04344f 71%,#04344f 100%); /* W3C */
	border:1px solid #3c3c3c;
	text-shadow: 1px 1px 1px #000;
	position:absolute;
	z-index: 100;
}

